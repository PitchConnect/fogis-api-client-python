            "example": 5
          },
          "domaruppdragstatusnamn": {
            "type": "string",
            "description": "Referee assignment status name",
            "example": "Tilldelat"
          },
          "personnamn": {
            "type": "string",
            "description": "Person name",
            "example": "John Doe"
          },
          "telefon": {
            "type": "string",
            "description": "Phone number",
            "example": ""
          },
          "mobiltelefon": {
            "type": "string",
            "description": "Mobile phone number",
            "example": "0700000000"
          },
          "telefonarbete": {
            "type": "string",
            "description": "Work phone number",
            "example": ""
          },
          "adress": {
            "type": "string",
            "description": "Address",
            "example": "Example Street 1"
          },
          "coadress": {
            "type": "string",
            "description": "Co-address",
            "example": ""
          },
          "epostadress": {
            "type": "string",
            "description": "Email address",
            "example": "john.doe@example.com"
          },
          "land": {
            "type": "string",
            "description": "Country",
            "example": "Sverige"
          },
          "namn": {
            "type": "string",
            "description": "Name",
            "example": "John Doe"
          },
          "postnr": {
            "type": "string",
            "description": "Postal code",
            "example": "12345"
          },
          "postort": {
            "type": "string",
            "description": "Postal city",
            "example": "Example City"
          }
        }
      }
    },
    "domarefargodkannarapport": {
      "type": "boolean",
      "description": "Whether the referee can approve the report",
      "example": false
    },
    "domarrapporteringTillaten": {
      "type": "boolean",
      "description": "Whether referee reporting is allowed",
      "example": false
    },
    "noteringfranlag1": {
      "type": "string",
      "description": "Notes from home team",
      "example": ""
    },
    "noteringfranlag2": {
      "type": "string",
      "description": "Notes from away team",
      "example": ""
    },
    "noteringfrandomare": {
      "type": "string",
      "description": "Notes from referee",
      "example": ""
    },
    "tavlingFriaByten": {
      "type": "boolean",
      "description": "Whether the competition allows free substitutions",
      "example": true
    },
    "tavlingDomareMojliggorHamtningAvFrammandeSpelareTillMatchtrupp": {
      "type": "boolean",
      "description": "Whether the referee can fetch foreign players to match squad",
      "example": true
    },
    "tavlingAntalTimmarInnanMatchForTruppAdministration": {
      "type": "integer",
      "description": "Number of hours before match for squad administration",
      "example": 0
    },
    "tavlingDomareKravForekomstAvAnsvarigLagledareIGodkandDomarrapport": {
      "type": "boolean",
